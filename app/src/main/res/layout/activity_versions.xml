<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_versions"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/guanyu"
    tools:context="example.com.myexample.activity.VersionsActivity">

    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:background="@mipmap/tou">

        <ImageView
            android:id="@+id/back"
            android:layout_width="50px"
            android:layout_height="50px"
            android:layout_centerVertical="true"
            android:layout_marginLeft="45px"
            android:src="@mipmap/back1" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="编辑资料"
            android:textColor="#ffffff"
            android:textSize="40px" />
    </com.zhy.autolayout.AutoRelativeLayout>

    <TextView
        android:id="@+id/vs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:text="当前版本"
        android:textColor="#000000"
        android:textSize="32px" />

    <TextView
        android:id="@+id/vscode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/vs"
        android:layout_centerHorizontal="true"
        android:text="ssssss" />

    <TextView
        android:id="@+id/vdbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/vscode"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20px"
        android:text="检测更新"
        android:textColor="#000000"
        android:textSize="40px" />
</com.zhy.autolayout.AutoRelativeLayout>
